@model IEnumerable<SweetStack.DomainObjects.SweetTest>

@{
    ViewBag.Title = "Index";
}

<h2>Finished Tests</h2>

<table class="table">
    <tr>
        <th>
            ID
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Timestamp)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Name
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Timestamp)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.Name }) 
        </td>
    </tr>
}

</table>
